				<true/>
				<key>CFBundleIconFiles</key>
				<array>
					<string>icon-green-76</string>
				</array>
			</dict>
			<key>icon-yellow</key>
			<dict>
				<key>UIPrerenderedIcon</key>
				<true/>
				<key>CFBundleIconFiles</key>
				<array>
					<string>icon-yellow-152</string>
				</array>
			</dict>
			<key>icon-purple</key>
			<dict>
				<key>UIPrerenderedIcon</key>
				<true/>
				<key>CFBundleIconFiles</key>
				<array>
					<string>icon-purple-76</string>
				</array>
			</dict>
		</dict>
  </dict>

</dict>
</plist>
